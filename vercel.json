{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/auth.routes.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/comments.routes.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ] 
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/experience.routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/getSelf.routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/hero.routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/mails.routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/projects.routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "/src/routes/routes.js" ,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
