{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/auth.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/comments.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/experience.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/getSelf.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/hero.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/mails.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  },
  {
    "src": "/api/(.*)",
    "dest": "src/routes/projects.routes.js",
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
  }
  ]
}
