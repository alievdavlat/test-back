{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
        "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/media/(.*)",
      "dest": "public/$1",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/api/(.*)",
      "dest": "src/routes/routes.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
    ]
    }
  ]
}